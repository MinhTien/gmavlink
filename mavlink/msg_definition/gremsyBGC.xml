	 <!-- MESSAGE IDs 150 - 240: Space for custom messages in individual projectname_messages.xml files -->
     <messages>
	 		  
		  <message id="151" name="PPM_CHAN_VALUES">
               <description>send PPM values of TILT, PAN, ROLL, MODE channels</description>
               <field type="float" name="tilt">ppm tilt value</field>	 
			   <field type="float" name="pan">ppm pan value</field>	
			   <field type="float" name="roll">ppm roll value</field>	 
			   <field type="float" name="mode">ppm mode value</field>	  			   
          </message>
	 
