		  <message id="153" name="RC_SIMULATION">
               <description>simulate rc channels</description>
               <field type="int16_t" name="tilt">tilt value in degree</field>
			   <field type="int16_t" name="roll">roll value in degree</field>	
			   <field type="int16_t" name="pan">pan value in degree</field>	  	 	   			   
          </message>
		  
		  <message id="154" name="IMU_CALIB_REQUEST">
               <description>send calib imu request</description>  		
			   <field type="uint8_t" name="calib_type">imu calib type Gyro or Accelerometer</field>
			   <field type="uint8_t" name="acc_calib_mode">this argument is optional, only used when calib type is ACC</field>
          </message>		 

  		  <message id="155" name="SYSTEM_STATUS">
               <description>send debug values</description>  		
			   <field type="float" name="battery_voltage">current battery voltage</field>
			   <field type="uint8_t" name="imu_calib">imu calib status</field>
